	[0x0000]
	{
		from = "What? You found the Crystwind Hammer Stone? By the gods, that's it! Well done, friend! I admit I was not confident that it would be found. Perhaps the Society really will have a chance to prove my theory... Hmmm.";
		to = "Was? Ihr habt den Hammerstein von Kristallwind gefunden? Bei den Göttern, das ist er! Gut gemacht, mein Freund! Ich gebe zu, dass ich nicht sehr zuversichtlich war, dass er jemals gefunden würde. Vielleicht wird die Gemeinschaft tatsächlich die Möglichkeit haben, meine Theorie zu bestätigen ... Hmmm.";
	}
	[0x0000]
	{
		from = "Find the Diamond Runestone in the ruins near the town of Crystwind.";
		to = "Suchen Sie die Diamantrune in den Ruinen nahe der Stadt Kristallwind.";
	}
	[0x0000]
	{
		from = "Town of Crystwind, 45";
		to = "Stadt Kristallwind, 45";
